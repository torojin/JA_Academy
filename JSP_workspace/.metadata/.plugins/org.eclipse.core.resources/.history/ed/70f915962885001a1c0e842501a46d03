package com.ja.rubato.vo;

import java.util.*;

public class BoardVo {
	//field 생성
	private int b_no;
	private int m_no;
	private String b_title;
	private String b_content;
	private String b_count;
	private Date b_writedate;
	
	//constructor 생성
	public BoardVo() {
		super();
	}
	public BoardVo(int b_no, int m_no, String b_title, String b_content, String b_count, Date b_writedate) {
		super();
		this.b_no = b_no;
		this.m_no = m_no;
		this.b_title = b_title;
		this.b_content = b_content;
		this.b_count = b_count;
		this.b_writedate = b_writedate;
	}
	
	
}
