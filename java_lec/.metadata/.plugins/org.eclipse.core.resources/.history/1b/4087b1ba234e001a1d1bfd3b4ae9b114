package Shape;
/*
 * +Triangle()
 * +Triangle(x:int, y:int)
 * +getSize():double
 */
public class Triangle extends Shape{
	private int x,y;
	//디폴트 생성자 생성
	public Triangle() {
		super();
	}
	//매개변수 생성자 생성
	public Triangle(int x, int y) {
		super();
		this.x=x;
		this.y=y;
	}
	public double getSize() {
		return x*y;
	}

}
