package Shape;
//정적 바인딩을 동적 바인딩으로 바꿔보자
public class ShapeMain {

	public static void main(String[] args) {
		//정적 바인딩
		Rectangle ob1=new Rectangle(4,5);
		System.out.println("사각형의 넓이  : "+ob1.getSize());//20
		
		Triangle ob2=new Triangle(4,5);
		System.out.println("삼각형의 넓이  : "+ob2.getSize());//10.5
		
		//동적 바인딩
		//부모 클래스의 참조변수로 자식 객체 생성
		
		//객체 매개변수 전달
		Shape sh=new Rectangle(4,5);
		System.out.println("사각형의 넓이  : "+sh.getSize());//20
		
		Shape sh1=new Triangle(3,7);
		System.out.println("삼각형의 넓이  : "+sh1.getSize());//10.5
		
		//Shape method 호출
		sh.getSize();
	}
	

}
