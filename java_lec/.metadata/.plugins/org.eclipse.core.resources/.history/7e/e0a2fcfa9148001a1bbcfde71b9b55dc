package forex;
/**
 *
 * for(초기값; 조건식; 증감식) {} ==> for(int i=0; i<10; i++)
 * 
 * 과제
 * 
 * 홀수 합을 출력하세요
 */
public class ForEx1 {

	public static void main(String[] args) {
		
		int sum = 0;
		int odd = 0;
		
		for(int i=1; i<=10; i++) {		// 후위연산자 -> 중괄호의 실행이 끝나고나면 증가
			sum += i; 
			switch(i % 2) {
			case 1 :
				odd += i;
				System.out.println(i);
				break;
			case 0 :
				break;
			}
		}
		
		System.out.println("합계 : "+sum);
		System.out.println("홀수 합 : "+odd);
	}

}
