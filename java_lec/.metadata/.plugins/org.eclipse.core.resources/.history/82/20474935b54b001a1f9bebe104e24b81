package array;
// 2차원 배열, 가변배열
// 2차원 배열 : 표를 나타내는 것
//데이터의 사이즈가 명확하게 정해져 있는 경우 2차원 배열을 쓴다
public class ArrayEx6 {
	//3행 4열로 나오게 하는 것
	public static void main(String[] args) {
		int [][] num=new int[][] {
								 {10, 40, 70, 100},
								 {20, 50, 80, 110},
								 {30, 60, 90, 120}
								 };
	}

}
