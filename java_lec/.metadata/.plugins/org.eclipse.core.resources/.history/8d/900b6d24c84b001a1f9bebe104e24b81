package object;
/* 클래스 다이어그램 표기
 * -(private) +(public) #(protected)
 *  위에 이름
 *  중간 변수
 *  괄호가 있는건 함수
 *  
 *  클래스 명 : Person
 *  +name : String
 *  +age : int
 *  +score : float
 *  +setPerson(name:String, a:int, s:float) : void
 *  +viewPerson():void
 *
 */
//괄호가 붙으면 함수
class Person{
	public String name;
	public int age;
	public float score;	
}


public class ObjectEx1 {
//public : 접근 지정자
	
	public static void main(String[] args) {

	}

}
