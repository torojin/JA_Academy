package instance;

//class »ý¼º
class A{}
class B extends A{}
class C extends A{}
public class InstanceofEx1 {

	public static void main(String[] args) {
		A a=new A();
		B b=new B();
		
		System.out.println(b instanceof A);
		System.out.println(a instanceof C);
		System.out.println(b instanceof C); //error
	}

}
