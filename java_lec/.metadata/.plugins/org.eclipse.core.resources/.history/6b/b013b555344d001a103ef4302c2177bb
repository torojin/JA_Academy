package constructor;

public class ConstructorEx2 {
	
	//생성자
	public ConstructorEx2(String str, int n) {
		
		this();//this 생성자 호출 = 생성자를 자기자신의 생성자를 호출하겠다.
		System.out.println(str+" "+n+" constructor");
	}

	public static void main(String[] args) {
		//객체 생성 매개변수 전달
		new ConstructorEx2("가나다",10);
	}

}
