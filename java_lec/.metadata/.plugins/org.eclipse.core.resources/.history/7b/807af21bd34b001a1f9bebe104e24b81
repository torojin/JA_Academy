package object;

public class ObjectEx2 {

	//시작은 여기서
	//그 외의 객체는 User.java에서
	//2개의 메서드를 만들어야함 setUser,getUser
	public static void main(String[] args) {
		User ob=new User();
		ob.setUser("진달래", 97,55,79);
		//출력을 하기 위해
		System.out.println(ob.getUser());
		
		//--출력
		//이름은 진달래이고, 총점은 231점입니다
	}

}
