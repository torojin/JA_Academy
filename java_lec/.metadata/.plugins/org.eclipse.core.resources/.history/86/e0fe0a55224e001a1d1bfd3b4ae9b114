package Shape;
/*
 * +Triangle()
 * +Triangle(x:int, y:int)
 * +getSize():double
 */
public class Triangle {
	//叼弃飘 积己磊 积己
	public Triangle() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Triangle(int x, int y) {
		super();
		// TODO Auto-generated constructor stub
	}
	public double getSize() {
		return x*y;
		
	}

}
