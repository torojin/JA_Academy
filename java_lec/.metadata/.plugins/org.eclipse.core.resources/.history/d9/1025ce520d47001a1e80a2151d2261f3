package step01_기초;
/**
 * 
 * @author admin
 * float 변수를 만들고 75.3
 * double 변수를 만들고 89.53을 각각 대입한 후
 * 
 * system.out.print(); 를 한번만 사용하여 출력하세요
 * 
 * ---------출력------------
 * 국어 점수는 75.3점이고
 * 수학점수는 89.53점입니다
 */
public class Test3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		float a = 75.3f;
		double b = 89.53;
		
		System.out.print("국어 점수는 "+a+"점이고\n"+"수학점수는 "+b+"점입니다");
	}

}
