package object;

import java.util.Scanner;

public class ScannerEx {

	public static void main(String[] args) {
		//Scanner
		//입력
		Scanner sc=new Scanner(System.in);
		
		System.out.print("문자열 : ");
		String str=sc.nextLine();
		
		System.out.print("정수 : ");
		int a=sc.nextInt();
		
		System.out.print("실수 : ");
		double b=sc.nextDouble();
		System.out.print("문자 : ");
	}

}
