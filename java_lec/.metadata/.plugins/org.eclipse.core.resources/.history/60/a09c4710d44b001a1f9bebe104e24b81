package object;
/* 클래스 다이어그램 표기
 * -(private) +(public) #(protected)
 * +name:String
 * +k:int
 * +e:int
 * +m:int
 * 
 * +setUser(name:String, k:int, e:int, m:int) :void
 * +getTotal():int
 * +getUser():String //getUser는 getTotal 호출
 * 
 */
public class User {
	public String name;
	public int k;
	public int e;
	public int m;
	
	public void setUser(String n, int kor, int eng, int mat) {
		name=n;
		k=kor;
		e=eng;
		m=mat;
	}
	public String getUser() {
		System.out.println("이름은"+name+"이고, 총점은 "+tot+"점입니다.");
	}
	public int getTotal(int t) {
		int tot=t;
		return t=k+e+m;
	}
}
