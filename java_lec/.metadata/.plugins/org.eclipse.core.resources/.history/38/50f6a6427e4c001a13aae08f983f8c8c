package object;
/*
 * -base : int
 * -top : int
 * -height : int
 * 
 * +setTrapezoid(base:int, top:int, height:int):void
 * +calc():double <-- 사다리꼴의 넓이를 구해서 리턴(아랫변+윗변)*높이/2.0
 * +toString():String
 * 
 */
public class Trapezoid {

	//필드
	private int base;
	private int top;
	private int height;
	
	//setter
	public void setBase(int base) {
		this.base = base;
	}
	public void setTop(int top) {
		this.top = top;
	}
	public void setHeight(int height) {
		this.height = height;
	}
	

	
}
