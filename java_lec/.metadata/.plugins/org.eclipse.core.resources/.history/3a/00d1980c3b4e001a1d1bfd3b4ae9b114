package test22;
/*
(2) 파일명 : Salary      
-name:String
-salary:int
+Salary()
+Salary(name:String, salary:int)
+toString():String         ==>  이름, 연봉출력
 */
public class Salary {
	//filed(변수)
	private String name;
	private int salary;
	
	//default 생성자
	public Salary(){
		super();
	}

	public Salary(String name, int salary) {
		super();
	}
	//toString
	@Override
	public String toString() {
		return "Salary [name=" + name + ", salary=" + salary + "]";
	}

}
