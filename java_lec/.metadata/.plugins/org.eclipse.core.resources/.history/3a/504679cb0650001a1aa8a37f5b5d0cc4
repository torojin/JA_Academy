package weekendstudy;
//자식 클래스
public class Mother extends Family implements Job{
	//default constructor
	public Mother() {
		super();
	}
	//parameter constructor
	public Mother(String name) {
		super(name);
	}
	//interface 상속 / 오버라이딩 /리턴값 필요
	@Override
	public String work() {
		return super.toString()+"엄마는 집안일을 한다";
	}
	
}
