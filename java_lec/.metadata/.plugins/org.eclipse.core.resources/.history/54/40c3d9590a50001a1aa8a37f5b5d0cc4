package weekendstudyarray;
//부모 클래스 
public class Family {
	//field(variable) 생성
	private String name;
	
	//default constructor
	public Family() {
		super();
	}
	//parameter constructor
	public Family(String name) {
		super();
		this.name=name;
	}
	@Override
	public String toString() {
		return "Family [name=" + name + "]";
	}
	
	public String getName() {
		return "이름 : "+name;
	}
	public void setName(String name) {
		this.name = name;
	}
	

}
