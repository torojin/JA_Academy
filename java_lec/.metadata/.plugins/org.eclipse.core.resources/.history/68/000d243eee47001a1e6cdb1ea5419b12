package 자바문제1;

public class Ex2 {

	public static void main(String[] args) {
		String name = "이효리";
		String dept = "개발부";
		String position = "대리";
		int sal = 1500000;
		
		System.out.println("이름 : "+name);
		System.out.printf("부서 : %s", dept);
		System.out.print("직위 : "+position+"\n급여 : "+sal+"원");
	}

}
