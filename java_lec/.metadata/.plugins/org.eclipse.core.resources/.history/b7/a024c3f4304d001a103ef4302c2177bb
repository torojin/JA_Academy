package constructor;

public class ConstructorEx1 {
	//생성자
	//접근지정자
	//리턴이 아예 없다
	public ConstructorEx1() {
		System.out.println("default constructor");
	}
	//함수
	public void ConstructorEx1() {
		
	}
		
	public static void main(String[] args) {
		//객체 생성
		//한번만 쓰고 말거면 아래처럼
		new ConstructorEx1();
	}

}
