package object;
//객체 복습
/*
 * 클래스명 : Emp
 * -name : String
 * -dept : String
 * -salary : int
 * 
 * +setEmp(name:String, dept:String, salary:int):void
 * +printEmp():void
 * --홍길동 사원은 개발부에 근무하며 1520000원의 급여를 받아요
 */

public class ObjectTest1 {
	//filed
	private String name;
	private String dept;
	private int salary;
	
	//setter
	public void setEmp(String name, String dept, int salary) {
		this.name=name;
		this.dept=dept;
		this.salary=salary;
	}
	
	public void printEmp() {
		name+"사원은 "+dept+"에 근무하며 "+salary+"원의 급여를 받아요"
	}
	
	
	public static void main(String[] args) {
		ObjectTest1 ob=new ObjectTest1();
		ob.setEmp("홍길동","개발부",1520000);
		ob.printEmp();
		

	}

}
