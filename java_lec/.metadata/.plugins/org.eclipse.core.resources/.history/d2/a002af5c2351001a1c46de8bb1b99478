package tv2;
//결합도가 높은 코드, 다형성 이용
//부모 클래스
public class TvUser {

	public static void main(String[] args) {
		//배열처리
		String com=args[0];
		//tv 객체 생성
		Tv tv=null;
		
		if(com.equals("LG")||com.equals("lg")) {//com=="LG"
			//Object 생성
			LgTv tv=new LgTv();
			tv.powerOn();
			tv.powerOff();
		}else if(com.equals("SAMSUNG")||com.equals("samsung")) {//com=="SAMSUNG"
			SamsungTv tv=new SamsungTv();
			tv.soundUp();
			tv.soundDown();
		}else {
			System.out.println("error");
		}
		tv.powerOn();
		tv.powerOff();
		tv.soundUp();
		tv.soundDown();
	}

}
