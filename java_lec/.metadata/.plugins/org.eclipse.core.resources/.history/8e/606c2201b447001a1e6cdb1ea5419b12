package step02_연산자;

public class OperatorEx {

	public static void main(String[] args) {
		
		System.out.println(5/3); // 몫
		System.out.println(5%3); // 나머지
		
		int a = 5;
	
		System.out.println(a == 5); // a랑 5랑 같은가? true (5==a) true
		System.out.println(a >= 5); // a가 5보다 크거나 같은가
		System.out.println(a != 5); // a가 5가 아닌가? false
		
		String str = "korea";
		System.out.println(str=="korea");
		
	}

}
