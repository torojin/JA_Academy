package instance;
//abstract class(추상 클래스) 생성
abstract class Employee{
	String name;
	int salary;
	//default constructor 
	public Employee() {
		super();
	}
	//parameter constructor
	public Employee(String name, int salary) {
		super();
		this.name = name;
		this.salary = salary;
	}
	//abstract method선언
	abstract public int getBonus();
	
}

public class InstanceofEx2 {

	public static void main(String[] args) {

	}

}
