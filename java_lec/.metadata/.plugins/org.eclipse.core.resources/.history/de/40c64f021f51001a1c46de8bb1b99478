package tv1;
//자식 클래스 상속
public class LgTv {
	//전원 Method 선언
	public void powerOn() {
		System.out.println("LG TV-전원을 켠다");
	}
	public void powerOff() {
		System.out.println("LG TV-전원을 끈다");
	}
	//volume Method 선언
	public void volumeUp() {
		System.out.println("LG TV-소리를 높인다");
	}
	public void vloumeDown() {
		System.out.println("LG TV-소리를 줄인다");
	}
}
