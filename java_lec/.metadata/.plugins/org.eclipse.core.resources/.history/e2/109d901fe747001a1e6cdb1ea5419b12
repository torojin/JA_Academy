package switchex;
/**
 * 
 * @author user
 * 파이썬에는 switch문이 없음
 */
public class SwitchEx1 {

	public static void main(String[] args) {
		int score = 95;
		char grade;
		
		switch(score/10) { // 괄호 안(조건식)에는 정수값
		case 10:
		case 9:
			grade = 'A';
			break;
		case 8:
			grade = 'B';
			break;
		case 7:
			grade = 'C';
			break;
		case 6:
			grade = 'D';
			break;
			default:
				grade = 'F';
		}
		
		System.out.println("점수는 "+score+"점이고 학점은 "+grade+"입니다");
	}

}
