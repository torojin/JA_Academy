package constructor;

public class ConstructorEx1 {
	//생성자
	//제어는 이동해도 리턴값의 자료형을 표시하지 않는다
	//접근지정자 빼고 이름만 쓰는 경우가 있다.
	public ConstructorEx1() {
		System.out.println("default constructor");
	}
	//함수 리턴값의 자료형이 없는것
	//제어만 이동하는 리턴을 사용하는 것
	//void자료형
	public void ConstructorEx11() {
		
	}
		
	public static void main(String[] args) {
		//객체 생성
		//한번만 쓰고 말거면 아래처럼
		new ConstructorEx1();
	}

}
