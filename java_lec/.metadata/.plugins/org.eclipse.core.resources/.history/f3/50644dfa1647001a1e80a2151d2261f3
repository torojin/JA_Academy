package step02_연산자;
/**
 * 
 * @author admin
 * parseInt : 키보드에서 입력된 값을 숫자로 변환
 */
public class ParseInt {

	public static void main(String[] args) {
		String a = "100";	// 문자열 100
		String b = "200";
		
		int a1 = Integer.parseInt(a);	//"100"을 숫자로 바꿔줌
		int b1 = Integer.parseInt(b);
		
		System.out.println(a+b); // 문자열의 연결
		System.out.println(a1+b1);
	}

}
