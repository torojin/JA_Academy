package Shape;
/*
 * +Rectangle()
 * +Rectangle(x:int, y:int)
 * +getSize():double
 */
public class Rectangle extends Shape{
	//디폴트 생성자 생성
	public Rectangle() {
		super();
	}
	//매개변수 생성자 생성
	public Rectangle(int x, int y) {
		super(x,y);
	}
	
	@Override
	public double getSize() {
		return getX()*getY();
	}
}
