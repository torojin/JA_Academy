package step01_기초;

/**
 * 
 * @author admin
 * 제어문자
 * %s : String, char자료형을 가지는 문자열
 * %c : char형을 문자열(보통 이것 사용)
 * %f : float, double 실수형 자료형
 * 
 */
public class DataType2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name = "홍길동";
		char gender = '남';
		double score = 85.3;
		float age = 25;
		double tall = 183.4;
		
		System.out.printf("이름 : %s\n", name); // printf로 출력시 서식문자(제어문자)필요
		System.out.printf("성별 : %c\n", gender);
		System.out.printf("점수 : %f\n", score);
		System.out.printf("나이 : %f\n", age);
		System.out.printf("신장 : %f\n", tall);
		
	}

}
